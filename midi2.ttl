midi2:NoteOff1
	a rdfs:Class ;
	rdfs:subClassOf midi2:Midi1ChannelMessage ;
	rdfs:label "MIDI1 note off" ;
	rdfs:comment "MIDI1 note off message." ;
	midi2:statusMask "80"^^xsd:hexBinary ;
	midi2:chunk [
		midi2:byteNumber 0 ;
		midi2:property midi2:noteNumber
	] , [
		midi2:byteNumber 1 ;
		midi2:property midi2:velocity7
	] .

